{
  "configVersion": "v3",
  "app": {
    "appId": 1100445,
    "appName": "HAMGIS测亩",
    "appType": "app",
    "version": {
      "code": 2,
      "name": "1.0.2"
    },
    "icon": "icon.png",
    "vender": "BI4MIB",
    "description": "专业GIS土地面积测量应用"
  },
  "permissions": [
    "data:os.device.info",
    "device:os.geolocation",
    "device:os.network",
    "device:os.ble.connect",
    "device:os.vibrator",
    "device:os.local_storage",
    "device:os.barometer"
  ],
  "runtime": {
    "apiVersion": {
      "compatible": "3.0.0",
      "target": "3.0.0",
      "minVersion": "3.0"
    }
  },
  "targets": {
    "default": {
      "module": {
        "page": {
          "pages": [
            "page/measurement/index.page",
            "page/history/index.page",
            "page/settings/index.page",
            "page/projects/index.page",
            "page/project-detail/index.page",
            "page/map/index.page"
          ]
        },
        "app-side": {
          "path": "app-side/index"
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
